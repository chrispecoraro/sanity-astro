{
  "name": "@sanity/astro",
  "version": "2.0.1-beta.22",
  "description": "Official Sanity Astro integration",
  "scripts": {
    "dev": "vite build --watch",
    "build": "rimraf dist && vite build && npm run copyStudio && npm run copyDts",
    "release": "semantic-release",
    "prepublishOnly": "npm run build",
    "copyStudio": "cp -r src/studio dist/studio",
    "copyDts": "cp sanity-module.d.ts dist/sanity-module.d.ts"
  },
  "files": [
    "dist",
    "sanity-module.d.ts"
  ],
  "type": "module",
  "main": "./dist/sanity-astro.mjs",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "module": "./dist/sanity-astro.mjs",
      "default": "./dist/sanity-astro.mjs"
    },
    "./module": "./dist/sanity-module.d.ts",
    "./studio/studio-route.astro": "./dist/studio/studio-route.astro",
    "./studio/StudioComponent": {
      "types": "./dist/types/studio/StudioComponent.d.ts",
      "module": "./dist/studio/StudioComponent.tsx",
      "default": "./dist/studio/StudioComponent.tsx"
    }
  },
  "types": "./dist/types/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/sanity-io/sanity-astro.git",
    "directory": "packages/sanity-astro"
  },
  "author": "Sanity.io <hello@sanity.io>",
  "homepage": "https://www.sanity.io/plugins/sanity-astro",
  "bugs": {
    "url": "https://github.com/sanity-io/sanity-astro/issues"
  },
  "license": "MIT",
  "devDependencies": {
    "@sanity/semantic-release-preset": "^4.1.4",
    "astro": "^3.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rimraf": "^5.0.5",
    "semantic-release": "^21.1.1",
    "vite": "^4.4.9",
    "vite-plugin-dts": "^3.6.0"
  },
  "peerDependencies": {
    "@sanity/client": "^6.4.12",
    "astro": "^2.0.0 || ^3.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sanity": "*"
  },
  "keywords": [
    "astro-integration",
    "withastro",
    "sanity"
  ],
  "dependencies": {
    "@sanity/astro": "^2.0.1-beta.13",
    "@sanity/client": "^6.4.12",
    "@sanity/ui": "^1.8.2",
    "sanity": "*"
  },
  "release": {
    "extends": "@sanity/semantic-release-preset",
    "branches": [
      "main",
      {
        "name": "beta",
        "prerelease": true
      }
    ]
  },
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "engines": {
    "node": ">=18.14.1",
    "npm": ">=6.14.0"
  }
}
