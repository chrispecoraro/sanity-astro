{
  "extends": "astro/tsconfigs/base",
  "$schema": "https://json.schemastore.org/tsconfig",
  "include": ["./src/**/*.astro", "./src/**/*.ts", "sanity-module.d.ts"],
  "exclude": ["node_modules/*", "./vite*.ts", "dist"],
  "compilerOptions": {
    "rootDir": "src",
    "module": "ESNext",
    "target": "ESNext",
    "strict": false,
    "baseUrl": ".",
    "jsx": "preserve",
    "moduleResolution": "node"
  }
}