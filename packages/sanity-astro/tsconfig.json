{
  "extends": "astro/tsconfigs/base",
  "$schema": "https://json.schemastore.org/tsconfig",
  "include": ["./**/*.astro", "./**/*.ts", "sanity-module.d.ts", "./src/studio/*"],
  "exclude": ["node_modules/*", "./vite*.ts", "dist"],
  "compilerOptions": {
    "rootDir": "src",
    "module": "ESNext",
    "target": "ESNext",
    "strict": false,
    "baseUrl": ".",
    "jsx": "preserve",
    "moduleResolution": "node"
  }
}